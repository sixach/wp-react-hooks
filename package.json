{
	"name": "@sixa/wp-react-hooks",
	"version": "1.14.0",
	"description": "A collection of most used React hooks crafted for the sixa projects.",
	"keywords": [
		"sixa",
		"functions",
		"gutenberg",
		"hooks",
		"react",
		"wordpress"
	],
	"homepage": "https://github.com/sixach/wp-react-hooks#readme",
	"bugs": {
		"url": "https://github.com/sixach/wp-react-hooks/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/wp-react-hooks.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"sideEffects": false,
	"main": "build/index.js",
	"module": "build/index.js",
	"scripts": {
		"build": "cross-env BABEL_ENV=default NODE_ENV=production && rm -rf build && mkdir build && babel src --out-dir build --no-copy-ignored",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"prepublishOnly": "npm run format && npm run build",
		"docs": "documentation build src/** -f html --github -o docs",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format"
		]
	},
	"dependencies": {
		"@sixa/wp-block-utils": "1.2.0",
		"@wordpress/api-fetch": "6.9.0",
		"@wordpress/blob": "3.12.0",
		"@wordpress/block-editor": "8.6.0",
		"@wordpress/blocks": "11.11.0",
		"@wordpress/data": "6.12.0",
		"@wordpress/element": "4.10.0",
		"@wordpress/html-entities": "3.12.0",
		"@wordpress/i18n": "4.12.0",
		"@wordpress/url": "3.13.0",
		"lodash": "4.17.21",
		"react-geocode": "0.2.3",
		"use-deep-compare-effect": "1.8.1"
	},
	"devDependencies": {
		"@babel/cli": "7.18.9",
		"@babel/core": "7.18.9",
		"@babel/preset-env": "7.18.9",
		"@babel/preset-react": "7.18.6",
		"@wordpress/scripts": "22.5.0",
		"babel-loader": "8.2.5",
		"cross-env": "7.0.3",
		"documentation": "13.2.5",
		"husky": "7.0.4",
		"lint-staged": "12.5.0",
		"prettier": "npm:wp-prettier@2.6.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
